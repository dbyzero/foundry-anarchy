<div class="attribute-box anarchy-attribute" data-attribute="{{key}}">
  <a class="click-roll-attribute">
    <label for="data.attributes.{{key}}.value">{{localizeAttribute key}}</label>
  </a>
  <input class="larger" type="number" data-dtype="Number" maxlength="2"
    name="data.attributes.{{key}}.value" value="{{attribute.value}}" 
  />
</div>
