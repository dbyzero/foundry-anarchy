<a class="item anarchy-item anarchy-button click-roll-attribute-action flex-grow-0"
  title="{{localize labelkey}}"
  data-attribute="{{attribute}}" data-attribute2="{{attribute2}}"
  data-action-code="{{actionCode}}">
{{#if icon}}
  {{{icon}}}
{{else}}
  <label>{{localize labelkey}}</label>
{{/if}}
</a>